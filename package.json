{
  "name": "partiguiden-admin",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "dev": "concurrently --kill-others \"next dev\" \"pnpm https-proxy\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "pnpm prisma:generate",
    "format:write": "prettier --write '**/*.{js,ts,tsx,md,json}'",
    "format:check": "prettier -c '**/*.{js,ts,tsx,md,json}'",
    "https-proxy": "tsc dev-https-proxy.ts && node dev-https-proxy.js",
    "prisma:migrate": "dotenv -e .env.local prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "postcss": "8.4.29",
    "prisma": "^5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.11",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "concurrently": "^8.2.1",
    "dotenv-cli": "^7.3.0",
    "http-proxy": "^1.18.1",
    "prettier": "^3.0.3"
  }
}
