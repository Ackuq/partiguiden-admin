{
  "name": "partiguiden-admin",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "scripts": {
    "dev": "concurrently \"pnpm dev:web\" \"pnpm dev:proxy\" \"pnpm dev:db\"",
    "dev:web": "next dev",
    "dev:db": "docker-compose up -d",
    "dev:proxy": "tsc dev-https-proxy.ts -outDir .js_out && node .js_out/dev-https-proxy.js",
    "dev:seed": "tsc --resolveJsonModule --esModuleInterop -outDir .js_out prisma/seed.ts && dotenv -e .env.local node .js_out/seed.js",
    "dev:scraper": "tsc --skipLibCheck --outDir .js_out lib/scrapers/get-party-data.ts && node .js_out/get-party-data.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "pnpm prisma:generate",
    "typecheck": "pnpm tsc",
    "format:write": "prettier --write '**/*.{js,ts,tsx,md,json}'",
    "format:check": "prettier -c '**/*.{js,ts,tsx,md,json}'",
    "prisma:migrate": "dotenv -e .env.local prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "cheerio": "1.0.0-rc.12",
    "next": "13.4.19",
    "next-auth": "4.23.1",
    "pdfjs-dist": "^3.10.111",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/http-proxy": "1.17.12",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "autoprefixer": "10.4.15",
    "concurrently": "8.2.1",
    "dotenv-cli": "7.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "http-proxy": "1.18.1",
    "postcss": "8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "5.3.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  }
}
